{
  "name": "wayfare",
  "type": "module",
  "version": "0.1.1",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": "./src/index.ts"
  },
  "publishConfig": {
    "main": "./dist/index.mjs",
    "types": "./dist/index.d.mts",
    "exports": {
      "./package.json": "./package.json",
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "README.md",
    "package.json",
    "dist/**/*"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsdown",
    "test:types": "tsc --noEmit"
  },
  "keywords": [
    "WebGPU",
    "TypeGPU",
    "game",
    "engine",
    "3d",
    "graphics",
    "ecs"
  ],
  "license": "MIT",
  "author": {
    "name": "Iwo Plaza",
    "email": "iwo@iwoplaza.dev",
    "url": "https://iwoplaza.dev"
  },
  "peerDependencies": {
    "koota": "^0.5.0",
    "typegpu": "^0.7.0",
    "wgpu-matrix": "^3.4.0"
  },
  "dependencies": {
    "@loaders.gl/core": "^4.3.3",
    "@loaders.gl/gltf": "^4.3.3",
    "@loaders.gl/obj": "^4.3.3",
    "@loaders.gl/schema": "^4.3.3"
  },
  "devDependencies": {
    "@webgpu/types": "catalog:",
    "keyboardevent-codes": "1.0.0",
    "tsdown": "catalog:build",
    "koota": "^0.5.0",
    "typegpu": "catalog:",
    "unplugin-typegpu": "catalog:",
    "wgpu-matrix": "^3.4.0"
  }
}
